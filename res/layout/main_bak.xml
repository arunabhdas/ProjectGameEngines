<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	
	android:orientation="vertical" android:isScrollContainer="true"
	android:layout_height="wrap_content" android:layout_width="wrap_content">
	<LinearLayout android:id="@+id/widget29"

		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:orientation="vertical" android:layout_alignParentTop="true"
		android:background="@drawable/header" android:layout_centerHorizontal="true"
		android:gravity="center_horizontal">

		<Button android:gravity="center_horizontal"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:background="@drawable/homelogo3dsmall" />

	</LinearLayout>

	<ScrollView android:id="@+id/widget31" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_centerVertical="true"
		android:layout_weight="1"
		>
		<LinearLayout android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:orientation="vertical">
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:orientation="horizontal"
				android:layout_gravity="bottom|center" android:background="#877665">
				<ImageView android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:background="@drawable/promo"
					android:id="@+id/promImage"></ImageView>
			</LinearLayout>
			
			
			
			<LinearLayout android:layout_height="wrap_content"
				android:layout_width="fill_parent">
				<Gallery android:id="@+id/galleryTopGames"
				android:columnWidth="100px" android:layout_height="wrap_content"
					android:layout_width="fill_parent" android:focusable="true"
					android:layout_gravity="clip_vertical|clip_horizontal"
					android:fadingEdge="none" android:spacing="1px" 
					></Gallery>
			</LinearLayout>
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:orientation="horizontal"
				android:layout_gravity="bottom|center" android:background="@drawable/homepage_segcontrol">
			</LinearLayout>
			<FrameLayout android:layout_width="fill_parent"
				android:layout_height="wrap_content" android:layout_gravity="bottom|center"
				android:background="@drawable/results_table_bg"
				android:isScrollContainer="true">
